generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  reg String @id
  pass String
  name String?
  branch String?
  year Int?
  gender String?
  phNo String?
  Bus bus @relation(fields:[routeNo], references:[route])
  routeNo Int
  Stop stop @relation(fields: [point],references: [name])
  point String
}

model bus {
  name String @unique
  route Int @id
  driver String
  vechileNo String
  capacity Int
  strength Int
  stops stop[]
  user user[]
}

model stop {
  name String @id
  Bus bus @relation(fields:[busId], references:[route])
  busId Int
  user user[]
}
