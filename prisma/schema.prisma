generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  reg      String  @id
  pass     String
  branch   String?
  location String?
  name     String?
  busId    Int?
  stop     Int?
}

model bus {
  route Int @id
  driver String?
  stops stop[]
}

model stop {
  id Int @id
  Bus bus @relation(fields:[busId], references:[route])
  busId Int
  coords Json
}
